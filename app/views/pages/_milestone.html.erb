<div class="col-6 mt-3">
    <div class="card text-center bg-white">
        <div class="card-header bg-white">
            <h5 class="card-title">マイルストーン</h5>
        </div>
        <ul class="list-group list-group-flush">
         <% @stories.each do |s| %>
          <li class="list-group-item"><%= s.like_count %> <%= link_to s.title,story_path(s.id),class:'text-secondary' %> </li>
        </ul>

       <% end %>
    </div>
</div>
